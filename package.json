{
  "name": "@policar/react-redux-form",
  "version": "0.2.3",
  "scripts": {
    "prebuild": "npm run clean && npm run test",
    "build": "babel src --out-dir lib --ignore tests",
    "clean": "rimraf lib coverage",
    "lint": "npm run lint:js -- .",
    "lint:js": "eslint --ignore-path .gitignore",
    "prepublish": "npm run build",
    "test": "npm run test:single -- $(find src -path '*.spec.js')",
    "test:single": "cross-env NODE_ENV=test mocha --compilers js:babel-register src/tests/.setup.js",
    "test:watch": "npm run test -- --watch"
  },
  "pre-commit": {
    "run": [
      "lint",
      "test"
    ],
    "silent": true
  },
  "author": "Pavlin Poliƒçar <pavlin.g.p@gmail.com>",
  "keywords": [
    "react",
    "reactjs",
    "react-redux",
    "redux",
    "form"
  ],
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true,
      "es6": true
    },
    "plugins": [
      "react",
      "jsx-a11y"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "arrow-body-style": [
        2,
        "as-needed"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "import/imports-first": 0,
      "import/newline-after-import": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 2,
      "import/prefer-default-export": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "jsx-a11y/aria-props": 2,
      "jsx-a11y/heading-has-content": 0,
      "jsx-a11y/href-no-hash": 2,
      "jsx-a11y/label-has-for": 2,
      "jsx-a11y/mouse-events-have-key-events": 2,
      "jsx-a11y/role-has-required-aria-props": 2,
      "jsx-a11y/role-supports-aria-props": 2,
      "max-len": 0,
      "new-cap": [
        2,
        {
          "capIsNewExceptions": [
            "Immutable",
            "Map",
            "List",
            "Record",
            "OrderedMap",
            "Router",
            "Schema",
            "Set"
          ]
        }
      ],
      "newline-per-chained-call": 0,
      "no-console": 1,
      "no-use-before-define": 0,
      "prefer-template": 2,
      "react/jsx-filename-extension": 0,
      "react/jsx-no-target-blank": 0,
      "react/require-extension": 0,
      "react/self-closing-comp": 0,
      "require-yield": 0
    }
  },
  "license": "MIT",
  "main": "src/index.js",
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "invariant": "^2.2.1",
    "lodash": "^4.15.0",
    "react-dom": "^15.0.0-rc.2",
    "redux-actions": "^0.12.0",
    "reselect": "^2.5.3",
    "validator": "^5.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "^2.0.1",
    "enzyme": "^2.4.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "expect": "^1.20.2",
    "istanbul": "^0.4.5",
    "jsdom": "^9.5.0",
    "mocha": "^3.0.2",
    "pre-commit": "^1.1.3",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.0.0-rc.2",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-saga": "^0.11.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.5"
  },
  "peerDependencies": {
    "react": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-saga": "^0.11.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pavlin-policar/react-redux-form.git"
  },
  "bugs": {
    "url": "https://github.com/pavlin-policar/react-redux-form/issues"
  },
  "homepage": "https://github.com/pavlin-policar/react-redux-form#readme",
  "description": ""
}
