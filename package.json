    "redux",
    "form"
  ],
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "license": "MIT",
  "main": "src/index.js",
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "invariant": "^2.2.1",
